        <mxCell id="KSt5WhF_cTXbCiw3sWYh-113" value="1可能有同学要说了：" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" parent="KSt5WhF_cTXbCiw3sWYh-123" vertex="1">
